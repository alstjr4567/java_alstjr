
public class 연습장 {

	public static void main(String[] args) { //1~100까지 5의배수만 출력함
		//<1번>
		for(int i =1 ; i<=100 ; i++) {
			if(i%5==0) {
				System.out.println(i);
			}
		}
		int num=0;

		for(int i=1;i<=100;i++){
			if(i%5==0) {
				num=num+i;
			}
		}
		System.out.println("합계:"+num);
		
		int sum=0;
		for(int a = 1; a<=10 ; a++) {
			System.out.println(sum);
			sum=sum+a;
		}


	}
}
