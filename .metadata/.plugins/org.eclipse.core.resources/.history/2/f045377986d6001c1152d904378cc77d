
public class 연습장 {
	
	    public static void main(String[] args) {//1~100까지 5의배수만 출력함
	       for(int i =1 ; i<=100 ; i++) {
	    	   if(i%5==0) {
	    		  
	    		   
	    		   
	    		   int num=0;
	    			
	    			for(int a=1;a<=10;a++){
	    				if(a%2==1) {
	    					num=num+a;
	    				}
	    			}
	    			System.out.println(i+num);
	    		   
	    	   }
	       }
	
	    }
}