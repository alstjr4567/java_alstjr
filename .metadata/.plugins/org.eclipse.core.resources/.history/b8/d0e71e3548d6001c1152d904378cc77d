
public class Ex02_02_복합대입 {
	public static void main(String[] args) {
		
		int a=10, b=20, c=30, d=40;
		
		System.out.println("a:"+a);
		System.out.println();
		
		//a=a+3;
		a=+3;
		System.out.println("a:"+a);
		
		System.out.println("b:"+b);
		//b=b-5;
		b-=5;
		System.out.println("b:"+b);
		
	}

}
