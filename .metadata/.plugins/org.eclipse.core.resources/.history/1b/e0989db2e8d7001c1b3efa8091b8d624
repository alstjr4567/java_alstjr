
public class Ex04_06_2차원입력 {

	public static void main(String[] args) {
		
		int[][] arr = new int[2][3];		
		
		int i,j;
		for(i=0;i<2;i++) {
			for(j=0;j<3;j++) {
				System.out.println(arr[i][j]+" ");
			}
			System.out.println();
		}
//		arr[0][0]=10;
//		arr[0][0]=sc.nextInt();
//		arr[0][1]=sc.nextInt();
//		arr[0][2]=sc.nextInt();
//		arr[1][0]=sc.nextInt();
//		arr[1][1]=sc.nextInt();
//		arr[1][2]=sc.nextInt();
		
		for(i=0;i<2;i++) {
			for(j=0;j<3;j++) {
				System.out.println("입력:");
				arr[i][j]=sc.nextInt();
			}
		}
	}//main

}
