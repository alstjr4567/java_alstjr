
class Member{
	private String id;
	private String passwd;
	
	void setId(String id){
		this.id = id;
	}
	
	void setPasswd(String passwd){
		this.passwd = passwd;
	}
	
	String getId(){
		return id;
	}
	
	String getPasswd(){
		return passwd;
	}
}
