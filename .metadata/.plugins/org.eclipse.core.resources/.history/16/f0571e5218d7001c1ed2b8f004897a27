
public class Ex03_11_РЬСп_for {

	public static void main(String[] args) {
		
		int i,j;
		
		for(i=0;i<5;i++) {// i=0 1 2 3 4
			for(j=0;j<5;j++) {// i= 0 1 2 3 4
				System.out.println("*");
			}
			System.out.println();
		}
		
		for(i=0;i<5;i++) {// i=0 1 2 3 4
			for(j=0;j<=i;j++) {// i= 0 1 2 3 4
				System.out.println("*");
			}
			System.out.println();
		}
	}

}