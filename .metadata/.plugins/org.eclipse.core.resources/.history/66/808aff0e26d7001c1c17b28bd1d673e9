
public class Ex03_13_break_continue {

	public static void main(String[] args) {
		
		//1~10출력
		int i;
		for(i=1;i<=10;i++) {
			System.out.print(i+" ");
			if(i==5) {
				break; //반복문을 빠져나간다. 조건문x
			}
		}
		System.out.println();
		
		
		for(i=1;i<=10;i++) {
			System.out.println(i+" ");
			if(i==5) {
				continue; //
			}
			System.out.println(i+" ");
		}

		
		
	}//main

}
