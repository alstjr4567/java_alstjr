import java.util.Scanner;

public class Ex05_10_김민석 {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		int a = 1;
		int b = 0;
		
		System.out.print("값 : "); 
		a = sc.nextInt();
		
		System.out.print("값 : ");
		b = sc.nextInt();
		
		System.out.println(s(a,b));
		
		
		int result = 1;
		
		for(int i=0; i<b ;i++) {
			result = result * a;
		}
	
		System.out.println(result);
		

	}
	
	static int s(int a, int b) {
		if (b==0)
			return 1;
		else
			return a*s(a,b-1);

	}

}

//정수 2개입력
//int a,b;
//3 4: 3의 4승을 더하는것\
//5 3: 5의3승을 더하는것