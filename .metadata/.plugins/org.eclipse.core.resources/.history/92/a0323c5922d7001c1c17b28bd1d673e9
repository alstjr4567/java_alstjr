
public class Ex03_12_while {

	public static void main(String[] args) {
		
		
		int i;
		for(i=1;i<=10;i++) {
			System.out.println(i=" ");
			
		}
		
		System.out.println();
		
		i=1;
		while(i<=10) {
			System.out.println(i+" ");
			i++;
		}
		
	
	}

}

/*


for(초기식; 조건식; 증감식) {
	반복할 문장;
}

while(조건식) {
	반복할 문장;
	증감식;
}



*/