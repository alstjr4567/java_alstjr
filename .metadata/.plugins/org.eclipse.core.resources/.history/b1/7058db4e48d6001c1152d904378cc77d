
public class Ex02_02_복합대입 {
	public static void main(String[] args) {
		
		int a=10, b=20, c=30, d=40;
		
		System.out.println("a:"+a);
		
		//a=a+3;
		a=+3;
		System.out.println("a:"+a);
		
		System.out.println("b:"+b);
		//b=b-5;
		b-=5;
		System.out.println("b:"+b);
		
		c *= 2; //c=c*2
		System.out.println("c:"+c);
		
		System.out.println("d:"+d);
		d%=6; //d=d%6
		System.out.println("d:"+d);
	}

}
