class Calculator{
	int x;
	int y;
	//두 수의 덧셈을 리턴하는 add 메서드 정의
	static int add(int a,int b) {
		return a+b;
	}
}

public class Ex05_04_메서드 {

	public static void main(String[] args) {
		
		Calculator c1 = new Calculator();
		c1.x=100;
		c1.y=200;
		
		int result=c1.add(10,20);
		System.out.println("result:"+result);
		
	}

}
