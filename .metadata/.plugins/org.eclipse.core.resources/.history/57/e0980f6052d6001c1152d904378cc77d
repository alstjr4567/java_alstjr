
public class Ex02_04_관계논리 {
	public static void main(String[] args) {
		int a=10, b=20;
		
		boolean c = a >= b; //=은 뒤에다 써야함
		System.out.println("a>b:"+c);
		System.out.println(a>b);
		System.out.println(a==b);
		System.out.println(a!=b);
		
		boolean result = a>10 && b>10;
		System.out.println("result:"+result);
	}

}
